journal = {FEMS Microbiology Ecology},
keywords = {Arctic Regions,Carbon Isotopes,Carbon Isotopes: analysis,Conservation of Natural Resources,Euryarchaeota,Euryarchaeota: isolation {\&} purification,Euryarchaeota: metabolism,Geologic Sediments,Geologic Sediments: chemistry,Geologic Sediments: microbiology,Methane,Methane: analysis,Methane: metabolism,Methanobacterium,Methanobacterium: isolation {\&} purification,Methanobacterium: metabolism},
month = {jul},
