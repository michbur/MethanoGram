file = {:C$\backslash$:/Users/Marcin {\L}ukaszewicz/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kamagata, Mikami - 1991 - Isolation and Characterization of a Novel Thermophilic.pdf:pdf},
