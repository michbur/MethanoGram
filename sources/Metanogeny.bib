issn = {1466-5026},
journal = {International journal of systematic and evolutionary microbiology},
keywords = {16S,16S: genetics,Acetates,Acetates: metabolism,Anaerobiosis,Bacterial,Bacterial: genetics,Base Composition,Beer,Bioreactors,China,Culture Media,DNA Restriction Enzymes,DNA Restriction Enzymes: genetics,Hydrogen-Ion Concentration,Methane,Methane: metabolism,Methanosarcinaceae,Methanosarcinaceae: chemistry,Methanosarcinaceae: classification,Methanosarcinaceae: isolation {\&} purification,Methanosarcinaceae: physiology,Molecular Sequence Data,Nucleic Acid,Oxidoreductases,Oxidoreductases: genetics,Oxidoreductases: metabolism,RNA,Ribosomal,Sequence Homology,Sewage,Sewage: microbiology,Species Specificity},
