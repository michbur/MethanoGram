journal = {International journal of systematic and evolutionary microbiology},
keywords = {Aged, 80 and over,Archaea,Archaea: classification,Archaea: genetics,Archaea: isolation {\&} purification,Base Composition,DNA, Archaeal,DNA, Archaeal: genetics,Feces,Feces: microbiology,Genome, Archaeal,Humans,Male,Methane,Methane: metabolism,Molecular Sequence Data,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Sequence Analysis, DNA},
month = {aug},
number = {Pt 8},
pages = {1902--7},
pmid = {22859731},
title = {{Methanomassiliicoccus luminyensis gen. nov., sp. nov., a methanogenic archaeon isolated from human faeces.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22859731},
volume = {62},
