@article{Chong2002,
abstract = {A methanogen, strain AK-1, was isolated from permanently cold marine sediments, 38- to 45-cm below the sediment surface at Skan Bay, Alaska. The cells were highly irregular, nonmotile coccoids (diameter, 1 to 1.2 microm), occurring singly. Cells grew by reducing CO2 with H2 or formate as electron donor. Growth on formate was much slower than that on H2. Acetate, methanol, ethanol, 1- or 2-propanol, 1- or 2-butanol and trimethylamine were not catabolized. The cells required acetate, thiamine, riboflavin, a high concentration of vitamin B12, and peptones for growth; yeast extract stimulated growth but was not required. The cells grew fastest at 25 degrees C (range 5 degrees C to 25 degrees C), at a pH of 6.0-6.6 (growth range, pH 5.5-7.5), and at a salinity of 0.25-1.25 M Na+. Cells of this and other H2-using methanogens from saline environments metabolized H2 to a very low threshold pressure (less than 1 Pa) that was dependent on the methane partial pressure. We propose that the threshold pressure may be limited by the energetics of catabolism. The sequence of the 16S rDNA gene of strain AK-1 was most similar (98{\%}) to the sequences of Methanogenium cariaci JR-1 and Methanogeniumfrigidum Ace-2. DNA-DNA hybridization between strain AK-1 and these two strains showed only 34.9{\%} similarity to strain JR-1 and 56.5{\%} similarity to strain Ace-2. These analyses indicated strain AK-1 should be classified as a new species within the genus Methanogenium. Phenotypic differences between strain AK-1 and these strains (including growth temperature, salinity range, pH range, and nutrient requirements) support this. Therefore, a new species, Methanogenium marinum, is proposed with strain AK-1 as type strain.},
author = {Chong, Song C and Liu, Yitai and Cummins, Michael and Valentine, David L and Boone, David R},
file = {:D$\backslash$:/Storino Files/Storino (s.j.jablonski@gmail.com)/Staz{\_}2013 (ml@all4m.com)/Jab{\l}o{\'{n}}ski/MetanogenyPDF/M.marinum.pdf:pdf},
issn = {0003-6072},
journal = {Antonie van Leeuwenhoek},
keywords = {Alaska,Bacterial Typing Techniques,Cold Temperature,Culture Media,DNA, Ribosomal,DNA, Ribosomal: analysis,Geologic Sediments,Geologic Sediments: microbiology,Hydrogen,Hydrogen: metabolism,Kinetics,Methane,Methane: metabolism,Methanomicrobiaceae,Methanomicrobiaceae: classification,Methanomicrobiaceae: genetics,Methanomicrobiaceae: growth {\&} development,Methanomicrobiaceae: isolation {\&} purification,Nucleic Acid Hybridization,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Seawater,Seawater: microbiology},
month = {aug},
number = {1-4},
pages = {263--70},
pmid = {12448725},
title = {{Methanogenium marinum sp. nov., a H2-using methanogen from Skan Bay, Alaska, and kinetics of H2 utilization.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12448725},
volume = {81},
year = {2002}
