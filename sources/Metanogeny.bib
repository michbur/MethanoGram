file = {:C$\backslash$:/Users/Marcin {\L}ukaszewicz/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Imachi et al. - 2008 - Methanolinea tarda gen. nov., sp. nov., a methane-producing archaeon isolated from a methanogenic digester sludge.pdf:pdf},
issn = {1466-5026},
journal = {International journal of systematic and evolutionary microbiology},
keywords = {16S,16S: genetics,Archaeal,Archaeal: analysis,Base Composition,DNA,Methane,Methane: biosynthesis,Methanomicrobiales,Methanomicrobiales: classification,Methanomicrobiales: genetics,Methanomicrobiales: isolation {\&} purification,Methanomicrobiales: physiology,Molecular Sequence Data,Oxidoreductases,Oxidoreductases: genetics,Phenotype,RNA,Refuse Disposal,Refuse Disposal: methods,Ribosomal,Sequence Analysis,Sewage,Sewage: microbiology,Species Specificity},
