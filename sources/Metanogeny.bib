file = {:C$\backslash$:/Users/Marcin {\L}ukaszewicz/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rea et al. - 2007 - Methanobrevibacter millerae sp. nov. and Methanobrevibacter olleyae sp. nov., methanogens from the ovine and bovine.pdf:pdf},
issn = {1466-5026},
journal = {International journal of systematic and evolutionary microbiology},
keywords = {16S,16S: genetics,Animals,Archaeal,Archaeal: genetics,Cattle,Cattle: microbiology,DNA,Formates,Formates: metabolism,Methanobrevibacter,Methanobrevibacter: classification,Methanobrevibacter: genetics,Methanobrevibacter: growth {\&} development,Methanobrevibacter: metabolism,Molecular Sequence Data,Phylogeny,RNA,Ribosomal,Ribosomal: genetics,Rumen,Rumen: microbiology,Sheep,Sheep: microbiology},
