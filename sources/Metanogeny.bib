number = {3},
pages = {463--468},
title = {{Characterization of Three Thermophilic Strains of Methanothrix (‘ ‘Methanosaeta ”) thermophila sp. nov. and Rejection of Methanothrix (“Methanosaeta ”) thermoacetophila}},
url = {http://ijs.sgmjournals.org/content/42/3/463.abstract},
volume = {42},
year = {1992}
}
