file = {:D$\backslash$:/Storino Files/Storino (s.j.jablonski@gmail.com)/Staz{\_}2013 (ml@all4m.com)/Jab{\l}o{\'{n}}ski/MetanogenyPDF/M.aeolicus.pdf:pdf},
issn = {1466-5026},
journal = {International journal of systematic and evolutionary microbiology},
keywords = {16S,16S: genetics,Archaeal,Archaeal Proteins,Archaeal Proteins: analysis,Archaeal: chemistry,Archaeal: genetics,Archaeal: isolation {\&} purification,Base Composition,DNA,Genes,Geologic Sediments,Geologic Sediments: microbiology,Mediterranean Sea,Methanococcus,Methanococcus: chemistry,Methanococcus: classification,Methanococcus: isolation {\&} purification,Methanococcus: physiology,Molecular Sequence Data,Nucleic Acid,Pacific Ocean,Phylogeny,Proteome,Proteome: analysis,RNA,Ribosomal,Ribosomal: chemistry,Ribosomal: isolation {\&} purification,Seawater,Seawater: microbiology,Sequence Analysis,Sequence Homology,Temperature,Water Microbiology,rRNA},
