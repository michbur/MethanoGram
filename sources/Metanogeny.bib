file = {:D$\backslash$:/Storino Files/Storino (s.j.jablonski@gmail.com)/Staz{\_}2013 (ml@all4m.com)/Jab{\l}o{\'{n}}ski/MetanogenyPDF/M.maripaludis.pdf:pdf},
journal = {Arch Microbiol},
keywords = {autotroph - hydrogen,deltae - methane-producing cocci,genic bacteria - methanogen,methano-,methanococcus maripaludis - marine,methanogenium olentangyi -},
number = {2},
pages = {91--97},
title = {{Characterization of Methanococcus maripaludis sp. nov., a new methanogen isolated from salt marsh sediment}},
url = {http://link.springer.com/article/10.1007/BF00408015},
volume = {135},
year = {1983}
