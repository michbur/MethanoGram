issn = {1466-5034},
journal = {International journal of systematic and evolutionary microbiology},
keywords = {16S,16S: genetics,Archaeal,Archaeal: chemistry,Archaeal: genetics,Autotrophic Processes,Base Composition,Carbon Dioxide,Carbon Dioxide: metabolism,Cell Adhesion,Cluster Analysis,DNA,Flagella,Flagella: physiology,Hot Springs,Hot Springs: microbiology,Hot Temperature,Hydrogen,Hydrogen-Ion Concentration,Hydrogen: metabolism,Iceland,Locomotion,Methanococcales,Methanococcales: classification,Methanococcales: genetics,Methanococcales: isolation {\&} purification,Methanococcales: physiology,Molecular Sequence Data,Phylogeny,RNA,Ribosomal,Ribosomal: chemistry,Ribosomal: genetics,Seawater,Seawater: microbiology,Sequence Analysis,Sodium Chloride,Sodium Chloride: metabolism},
