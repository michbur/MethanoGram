pages = {1--15},
pmid = {17428301},
title = {{Biogeochemistry of methane and methanogenic archaea in permafrost}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17428301 https://academic.oup.com/femsec/article-lookup/doi/10.1111/j.1574-6941.2007.00315.x},
volume = {61},
