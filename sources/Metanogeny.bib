issn = {0723-2020},
journal = {Systematic and applied microbiology},
keywords = {16S,16S: chemistry,Base Sequence,Geologic Sediments,Geologic Sediments: microbiology,Methanosarcina,Methanosarcina: classification,Methanosarcina: growth {\&} development,Methanosarcina: isolation {\&} purification,Molecular Sequence Data,Phylogeny,RNA,Ribosomal,Water Microbiology},
month = {nov},
number = {3},
pages = {362--7},
title = {{Methanosarcina lacustris sp. nov., a new psychrotolerant methanogenic archaeon from anoxic lake sediments.}},
