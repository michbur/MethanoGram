isbn = {1466-5034 (Electronic)$\backslash$r1466-5026 (Linking)},
issn = {1466-5034},
journal = {International journal of systematic and evolutionary microbiology},
keywords = {16S,16S: genetics,Archaeal,Archaeal: genetics,Bacterial,Bacterial: genetics,Base Composition,Biofilms,Caves,Caves: microbiology,DNA,Lakes,Lakes: microbiology,Lipids,Lipids: chemistry,Methanosarcina,Methanosarcina: classification,Methanosarcina: genetics,Methanosarcina: isolation {\&} purification,Molecular Sequence Data,Nucleic Acid Hybridization,Phylogeny,RNA,Ribosomal,Romania,Sequence Analysis,Sulfur,Water Microbiology},
